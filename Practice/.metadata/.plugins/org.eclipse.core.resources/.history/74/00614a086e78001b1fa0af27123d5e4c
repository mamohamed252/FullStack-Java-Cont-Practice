package week5Sandbox.loops;

public class SandboxLoops {

	public static void main(String[] args) {
		
		forloopSample();
		
	}

	private static void forloopSample() {
		// int otheri= 1; // expression
		
		
		for (int i = 0; i<=5; i++) {
		
			for(int j=1; j<=2; ++j) {
				System.out.println(j + "*");
			}
		}
		
	}

}
